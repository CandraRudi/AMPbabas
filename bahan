    <script type="text/javascript">
var baseurl = 'https://www.babastudio.com/';
      // When the user scrolls the page, execute myFunction
      window.onscroll = function() {myFunction()};

      // Get the header
      var header = document.getElementById("header-menu");

      // Get the offset position of the navbar
      var sticky = header.offsetTop;

      // Add the sticky class to the header when you reach its scroll position. Remove "sticky" when you leave the scroll position
      function myFunction() {
        if (window.pageYOffset > sticky) {
          header.classList.add("sticky");
          header.classList.add("sub-nav");
        } else {
          header.classList.remove("sticky");
          header.classList.remove("sub-nav");
        }
      }
      function simpan_murid()
{
    var namalengkap = $("#namalengkap") . val();
    var email_add = $("#email_add") . val();
    var telp = $("#telp") . val();
    var tinggal = $('#tinggal option:selected') . val();
        // var tinggal    = $('#detail_lokasi option:selected').val();
    var materi = $('#materis option:selected') . val();
        // var detail_lokasi  = $('#detail_lokasi option:selected').val();
        // var detail_lokasi  = '';

    if (namalengkap === '' || email_add === '' || telp === '' || tinggal === '' || materi === '') {
        alert('Silahkan isi seluruh form.');
        return true;
    }

    $("#tombol_save1") . hide(); //hide submit button    
    $("#tombol_save2") . hide(); //hide submit button  
        //$("#hide_div_submit").hide(); //hide submit button          
        //$("#hide_div_submit").css("display","none"); 
        //$("#hide_div_submit").css("visibility","hidden"); 
    $("#LoadingImage") . show(); //show loading image
    jQuery . ajax({
        type :"POST" , // HTTP method POST or GET
        url : baseurl + "save", //Where to make Ajax calls
            // url: baseurl+"message/save" //Where to make Ajax calls
        data : 'nama=' + namalengkap + '&email=' + email_add + '&telp=' + telp + '&tinggal=' + tinggal + '&materi=' + materi + '&captcha=' + grecaptcha . getResponse(), //Form variables
        success : function (response) {                
                
                //alert(response);
                //return false;
            if (response != 'captcha fail') {
                    // $("#tombol_save").show(); 
                $("#LoadingImage") . hide(); //hide loading image
                    // $("#hasil_ajax").html(response);
                console . log(response);
                    //closeModal();
                window . location . replace("https://www.babastudio.com/terima-kasih");
                $('.google_recaptcha_fail_notification') . css('display', 'none');
            } else if (response == 'captcha fail') {
                    //alert(response);
                $('.google_recaptcha_fail_notification') . css('display', 'block');
            }
        },
            error : function (xhr, ajaxOptions, thrownError) {
                // $("#tombol_save").show(); //show submit button
            $("#LoadingImage") . hide(); //hide loading image
            alert(thrownError);
        }
    }
    );
}
    </script>
